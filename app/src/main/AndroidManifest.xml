        <activity android:name=".Threads.BasicThreadActivity" />
        <activity android:name=".Views.GridView.GridViewActivity" />
        <activity android:name=".Views.ListView.ListViewActivity" />
        <activity android:name=".Views.RV.RVActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

